var myLandbot;
function initLandbot() {
  if (!myLandbot) {
    var s = document.createElement('script');
    s.type = "module"
    s.async = true;
    s.addEventListener('load', function() {
      var myLandbot = new Landbot.Livechat({
        configUrl: 'https://storage.googleapis.com/chats.landbot.io/v3/H-1207951-U6R8YLUBF29DUIRK/index.json',
      });
    });
    s.src = 'https://cdn.landbot.io/landbot-3/landbot-3.0.0.mjs';
    var x = document.getElementsByTagName('script')[0];
    x.parentNode.insertBefore(s, x);
  }
}
</script>

<!-- CONSULTA LOS DATOS DE CLIENTE LOGEADO Y GUARDA VARIABLES EN LANDBOT -->
<script>
(function() {
  fetch('/identify_customer.php')
    .then(response => response.json())
    .then(data => {
      if (data.success) {
        // Si el cliente está logueado, guardar datos en Landbot
        Landbot.exec('landbot-custom-data', {
          email: data.email,
          firstname: data.firstname,
          id_customer: data.id_customer
        });
      } else {
        // Si NO está logueado, eliminar las variables de Landbot
        Landbot.exec('landbot-delete-data', ['email', 'firstname', 'id_customer']);
      }
    })
    .catch(err => {
      console.warn('No se pudo identificar al cliente:', err);
    });
})();
</script> 
